#include "scipp/core/element/@ELEMENT_INCLUDE@.h"
#include "scipp/variable/@OPNAME@.h"
#include "scipp/variable/transform.h"

using namespace scipp::core;

#cmakedefine GENERATE_OUT

namespace scipp::variable {

Variable @NAME@(const VariableConstView &var) {
  return transform(var, element::@OPNAME@, std::string_view("@OPNAME@"));
}

#ifdef GENERATE_OUT
Variable &@NAME@(const VariableConstView &var, Variable &out) {
  transform_in_place(out, var, assign_unary{element::@OPNAME@}, std::string_view("@OPNAME@"));
  return out;
}
#endif

} // namespace scipp::variable
